<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="main" prefHeight="504.0" prefWidth="1007.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StockSystems.TransactionController">
   <children>
      <HBox>
         <children>
            <MenuBar prefHeight="25.0" prefWidth="950.0">
               <menus>
                  <Menu mnemonicParsing="false" text="Add Cashier">
                     <items>
                        <MenuItem fx:id="aCash" mnemonicParsing="false" onAction="#Acash" text="Add Cashier" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Add Product">
                     <items>
                        <MenuItem fx:id="Apro" mnemonicParsing="false" onAction="#aPro" text="Add Product" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="View Cash Details">
                     <items>
                        <MenuItem fx:id="Cdetail" mnemonicParsing="false" onAction="#cdetail" text="View Cash Details" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="View Transaction">
                     <items>
                        <MenuItem fx:id="transac" mnemonicParsing="false" onAction="#Trans" text="View Transaction" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Inventory">
                     <items>
                        <MenuItem fx:id="inven" mnemonicParsing="false" onAction="#Inven" text="Inventory" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Update Stock">
                     <items>
                        <MenuItem fx:id="UpdateS" mnemonicParsing="false" onAction="#Updates" text="Update Stock" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Sales as per Product">
                     <items>
                        <MenuItem fx:id="sPerP" mnemonicParsing="false" onAction="#SperP" text="Sales as per Product" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Sales as per Day">
                     <items>
                        <MenuItem fx:id="SperD" mnemonicParsing="false" onAction="#Sperd" text="Sales as per Day" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <Button fx:id="lOut" mnemonicParsing="false" onAction="#Logout" prefHeight="25.0" prefWidth="101.0" text="Logout" />
         </children>
      </HBox>
      <AnchorPane id="AnchorPane" prefHeight="477.0" prefWidth="1007.0" style="-fx-background-color: abd;" styleClass="mainFxmlClass">
          
         <children>
            <Label layoutX="14.0" layoutY="14.0" prefHeight="38.0" prefWidth="231.0" text="TRANSACTIONS">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font></Label>
            <TextField fx:id="searchBill" alignment="CENTER" layoutX="307.0" layoutY="88.0" prefHeight="25.0" prefWidth="303.0" promptText="SEARCH BILL NO OR CASHIER ID" />
            <TableView fx:id="table" layoutX="172.0" layoutY="113.0" prefHeight="328.0" prefWidth="572.0">
              <columns>
                <TableColumn fx:id="a1" prefWidth="110.0" text="CASHIER ID" />
                <TableColumn fx:id="a2" prefWidth="95.0" text="BILL NO" />
                  <TableColumn fx:id="a3" prefWidth="95.0" text="AMOUNT" />
                  <TableColumn fx:id="a4" prefWidth="112.0" text="DATE" />
                  <TableColumn fx:id="a5" prefWidth="157.0" text="DATE AND TIME" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
